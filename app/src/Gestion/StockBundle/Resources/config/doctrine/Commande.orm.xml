<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Gestion\StockBundle\Entity\Commande" table="commande">
    <indexes>
      <index name="IDX_6EEAA67DC29D63C1" columns="idproduit_id"/>
      <index name="IDX_6EEAA67DB6418165" columns="idpharmacien_id"/>
      <index name="IDX_6EEAA67D7674E434" columns="idfournisseur_id"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="quantite" type="integer" column="quantite" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="date" type="date" column="date" nullable="false"/>
    <many-to-one field="idproduit" target-entity="Produit" fetch="LAZY">
      <join-columns>
        <join-column name="idproduit_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idpharmacien" target-entity="Pharmacien" fetch="LAZY">
      <join-columns>
        <join-column name="idpharmacien_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idfournisseur" target-entity="Fournisseur" fetch="LAZY">
      <join-columns>
        <join-column name="idfournisseur_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
