<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Gestion\StockBundle\Entity\Typevente" table="typevente">
    <unique-constraints>
      <unique-constraint name="UNIQ_2A09F5FF4A44E93" columns="idachete_id"/>
    </unique-constraints>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="typevente" type="string" column="typevente" length="60" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="pourcentage" type="string" column="pourcentage" length="60" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <many-to-one field="idachete" target-entity="Achete" fetch="LAZY">
      <join-columns>
        <join-column name="idachete_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
